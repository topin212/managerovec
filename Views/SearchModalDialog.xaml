<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="Managerovec.Views.SearchModalDialog" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Search by tag"
	Width="300"
	Height="131">
	<Grid>
		<TextBox
			Height="20"
			Width="274"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="8,38,0,0"
			x:Name="tagtextBox" />
		<TextBlock
			Text="Enter your tag here and press the button below"
			Width="274"
			Height="22"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="8,8,0,0" />
		<Button
			Content="Search for tag"
			Width="104"
			Height="23"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="178,66,0,0"
			x:Name="button1"
			Click="button1_Click" />
	</Grid>
</Window>